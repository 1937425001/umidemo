import {Card} from 'antd'
import React from 'react'

export default class Cards extends React.Component{
  constructor(props){
    super(props);
  }
  cardss(){
    return(
      <div className='card'>
        <div className='card-content'>
          {this.props.content}
        </div>
      </div>
    );
  }

  render() {
    return (
      <div>
      {this.cardss()}
      </div>
    );
  }
}
